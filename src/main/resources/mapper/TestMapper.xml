<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.sunyard.itp.mapper.TestMapper">
	<insert id="addTest" parameterType="com.sunyard.itp.entity.Test">
		insert into TBL_TEST
		(ID,name,birthday)
		values
		(#{id},#{name},#{birthday})
	</insert>
</mapper>